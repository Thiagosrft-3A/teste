function setup() {
  createCanvas(400, 400);
}

function draw() {
  background("white");
  fill("black");
  textSize(64);
  textAlign(CENTER, CENTER);

  let maximoY = height;
  let minimoY = 0;
  let palavraY = "Tinta";
  let quantidadeY = map(mouseY, 0, height, 1, palavraY.length);
  let parcialY = palavraY.substring(0, quantidadeY);
  text(parcialY, 200, 200);
}
